{
  "name": "bitable_linker",
  "version": "1.0.0",
  "description": "my starter",
  "license": "MIT",
  "scripts": {
    "start": "pm2 start pm2.config.js",
    "start:test": "pm2 start pm2.config.js --env test",
    "start:node": "node dist/main.js",
    "start:nodetest": "cross-env NODE_ENV=test node dist/main.js",
    "stop": "pm2 stop bitable_linker",
    "restart": "npm run build && pm2 restart bitable_linker",
    "build:client": "cd ./web && npm run build",
    "build:server": "nest build",
    "build": "npm run build:server && npm run build:client",
    "dev:server": "cross-env NODE_ENV=local nest start --watch",
    "dev:serverDebug": "cross-env NODE_ENV=local nest start --debug --watch",
    "dev:client": "cd ./web && npm run dev",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:debug": "cross-env NODE_ENV=local nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ecoding/helper.env": "*",
    "@ecoding/helper.fn": "*",
    "@ecoding/helper.got": "*",
    "@ecoding/helper.i18n": "*",
    "@ecoding/helper.is": "*",
    "@ecoding/helper.json": "*",
    "@ecoding/helper.qs": "*",
    "@ecoding/helper.url": "*",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/typeorm": "^9.0.1",
    "ali-oss": "^6.18.0",
    "axios": "^1.4.0",
    "bull": "^4.11.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "imageinfo": "^1.0.4",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "mysql2": "^3.3.3",
    "nacos": "^2.5.1",
    "nest-winston": "^1.9.2",
    "node-xlsx": "^0.23.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@ecoding/base.spec": "^0.0.6",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^28.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "concurrently": "^8.1.0",
    "jest": "^28.1.2",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.85.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
